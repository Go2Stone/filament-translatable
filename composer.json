{
    "name": "go2stone/filament-translatable",
    "description": "This package adds a way to interact with go2stone/translatable in Filament.",
    "homepage": "https://github.com/go2stone/filament-translatable",
    "license": "MIT",
    "authors": [
        {
            "name": "Go2Stone",
            "email": "info@go2stone.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^3.2",
        "laravel/framework": "^10.0|^11.0",
        "spatie/laravel-package-tools": "^1.16",
        "spatie/laravel-translatable": "^6.6"
    },
    "autoload": {
        "psr-4": {
            "Go2Stone\\FilamentTranslatable\\": "src/"
        },
        "files": [
            "src\\Helpers\\helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Go2Stone\\FilamentTranslatable\\FilamentTranslatableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
